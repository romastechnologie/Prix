{% extends 'base.html.twig' %}

{% block title %}Produit{% endblock %}

{% block content %}
    <h1>Produit</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ produit.id }}</td>
            </tr>
            <tr>
                <th>Designation</th>
                <td>{{ produit.designation }}</td>
            </tr>
            <tr>
                <th>Code</th>
                <td>{{ produit.code }}</td>
            </tr>
            <tr>
                <th>RefUsine</th>
                <td>{{ produit.refUsine }}</td>
            </tr>
            <tr>
                <th>ATaxe</th>
                <td>{{ produit.aTaxe }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ produit.description }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_produit_test_index') }}">back to list</a>

    <a href="{{ path('app_produit_test_edit', {'id': produit.id}) }}">edit</a>

    {{ include('produit_test/_delete_form.html.twig') }}
