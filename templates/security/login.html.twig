			<!--end::Page Custom Styles-->
			<!--begin::Global Theme Styles(used by all pages)-->
			<link href="{{ asset('admin/theme/html/demo1/dist/assets/plugins/global/plugins.bundlef552.css?v=7.1.8')}}" rel="stylesheet" type="text/css" />
			<link href="{{ asset('admin/theme/html/demo1/dist/assets/plugins/custom/prismjs/prismjs.bundlef552.css?v=7.1.8')}}" rel="stylesheet" type="text/css" />
			<link href="{{ asset('admin/theme/html/demo1/dist/assets/css/style.bundlef552.css?v=7.1.8')}}" rel="stylesheet" type="text/css" />
			<!--end::Global Theme Styles-->
			<!--begin::Layout Themes(used by all pages)-->
			<link href="{{ asset('admin/theme/html/demo1/dist/assets/css/themes/layout/header/base/lightf552.css?v=7.1.8')}}" rel="stylesheet" type="text/css" />
			<link href="{{ asset('admin/theme/html/demo1/dist/assets/css/themes/layout/header/menu/lightf552.css?v=7.1.8')}}" rel="stylesheet" type="text/css" />
			<link href="{{ asset('admin/theme/html/demo1/dist/assets/css/themes/layout/brand/darkf552.css?v=7.1.8')}}" rel="stylesheet" type="text/css" />
			<link href="{{ asset('admin/theme/html/demo1/dist/assets/css/themes/layout/aside/darkf552.css?v=7.1.8')}}" rel="stylesheet" type="text/css" />
