{% extends 'base.html.twig' %}

{% block title %}Client{% endblock %}

{% block content %}
    <h1>Client</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ client.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ client.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ client.prenom }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ client.adresse }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ client.email }}</td>
            </tr>
            <tr>
                <th>Telephone1</th>
                <td>{{ client.telephone1 }}</td>
            </tr>
            <tr>
                <th>Telephone2</th>
                <td>{{ client.telephone2 }}</td>
            </tr>
            <tr>
                <th>RaisonSociale</th>
                <td>{{ client.raisonSociale }}</td>
            </tr>
            <tr>
                <th>Ifu</th>
                <td>{{ client.ifu }}</td>
            </tr>
            <tr>
                <th>Rccm</th>
                <td>{{ client.rccm }}</td>
            </tr>
            <tr>
                <th>Sigle</th>
                <td>{{ client.sigle }}</td>
            </tr>
            <tr>
                <th>Denomination</th>
                <td>{{ client.denomination }}</td>
            </tr>
            <tr>
                <th>Statut</th>
                <td>{{ client.statut }}</td>
            </tr>
            <tr>
                <th>DateNais</th>
                <td>{{ client.dateNais ? client.dateNais|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_client_index') }}">back to list</a>

    <a href="{{ path('app_client_edit', {'id': client.id}) }}">edit</a>

    {{ include('client/_delete_form.html.twig') }}
{% endblock %}
